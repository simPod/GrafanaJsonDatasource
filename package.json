  "dependencies": {
    "@grafana/data": "^10.0.3",
    "@grafana/experimental": "^1.6.2",
    "@grafana/runtime": "^10.0.3",
    "@grafana/ui": "^10.0.3",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-virtualized-auto-sizer": "^1.0.20",
    "rxjs": "7.8.0",
    "ts-pattern": "^5.0.4",
    "tslib": "^2.6.1"
  },
  "homepage": "https://github.com/simPod/GrafanaJsonDatasource",
