{
  "name": "grafana-json-datasource",
  "description": "Grafana JSON datasource",
  "main": "index.js",
  "version": "0.6.3",
  "scripts": {
    "build": "webpack -c ./.config/webpack/webpack.config.ts --env production",
    "dev": "webpack -w -c ./.config/webpack/webpack.config.ts --env development",
    "lint": "eslint --cache --ignore-path ./.gitignore --ext .js,.jsx,.ts,.tsx .",
    "lint:fix": "yarn run lint --fix",
    "prettier:check": "prettier --list-different \"**/*.{ts,tsx,scss}\"",
    "prettier:write": "yarn run prettier:check --write",
    "sign": "npx --yes @grafana/sign-plugin@latest",
    "test": "jest --watch --onlyChanged",
    "test:ci": "jest --passWithNoTests --maxWorkers 4",
    "typecheck": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com:simPod/GrafanaJsonDatasource.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/simPod/GrafanaJsonDatasource/issues"
  },
  "devDependencies": {
    "@babel/core": "^7.22.5",
    "@grafana/eslint-config": "^6.0.0",
    "@grafana/toolkit": "^10.0.0",
    "@grafana/tsconfig": "^1.2.0-rc1",
    "@swc/core": "^1.3.65",
    "@swc/helpers": "^0.5.1",
    "@swc/jest": "^0.2.26",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.5.0",
    "@types/lodash": "^4.14.195",
    "@types/node": "^18.15.11",
    "@types/react-virtualized-auto-sizer": "^1.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.3",
    "eslint": "^8.30.0",
    "eslint-plugin-jsdoc": "^46.0.0",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-webpack-plugin": "^4.0.1",
    "fork-ts-checker-webpack-plugin": "^8.0.0",
    "glob": "^10.2.7",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "prettier": "^3.0.0",
    "replace-in-file-webpack-plugin": "^1.0.6",
    "sass": "^1.63.4",
    "sass-loader": "^13.3.1",
    "style-loader": "^3.3.3",
    "swc-loader": "^0.2.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.0.0",
    "webpack": "^5.86.0",
    "webpack-cli": "^5.1.4",
    "webpack-livereload-plugin": "^3.0.2"
