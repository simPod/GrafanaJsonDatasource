{
  "name": "grafana-json-datasource",
  "description": "Grafana JSON datasource",
  "main": "index.js",
  "version": "0.6.5",
  "scripts": {
    "build": "webpack -c ./.config/webpack/webpack.config.ts --env production",
    "dev": "webpack -w -c ./.config/webpack/webpack.config.ts --env development",
    "lint": "eslint --cache --ignore-path ./.gitignore --ext .js,.jsx,.ts,.tsx .",
    "lint:fix": "yarn run lint --fix",
    "prettier:check": "prettier --list-different \"**/*.{ts,tsx,scss}\"",
    "prettier:write": "yarn run prettier:check --write",
    "package": "cp -r dist/ simpod-json-datasource && zip simpod-json-datasource.zip simpod-json-datasource -r && rm -rfd simpod-json-datasource",
    "sign": "npx --yes @grafana/sign-plugin@latest",
    "test": "jest --watch --onlyChanged",
    "test:ci": "jest --passWithNoTests --maxWorkers 4",
    "typecheck": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com:simPod/GrafanaJsonDatasource.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/simPod/GrafanaJsonDatasource/issues"
  },
  "devDependencies": {
    "@babel/core": "^7.22.5",
    "@grafana/eslint-config": "^7.0.0",
    "@grafana/toolkit": "^10.0.0",
    "@grafana/tsconfig": "^2.0.0",
    "@swc/core": "^1.3.90",
    "@swc/helpers": "^0.5.1",
    "@swc/jest": "^0.2.26",
    "@swc/register": "^0.1.10",
    "@testing-library/dom": "^10.2.0",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^16.0.0",
    "@types/jest": "^29.5.7",
    "@types/lodash": "^4.14.195",
    "@types/node": "^20.8.7",
    "@types/react": "^18.2.31",
    "@types/react-dom": "^18.2.14",
    "@types/react-router-dom": "^5.2.0",
    "@types/react-virtualized-auto-sizer": "^1.0.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "copy-webpack-plugin": "^12.0.0",
    "css-loader": "^7.0.0",
    "eslint": "^8.30.0",
    "eslint-plugin-deprecation": "^3.0.0",
    "eslint-plugin-jsdoc": "^50.0.0",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-webpack-plugin": "^4.0.1",
    "fork-ts-checker-webpack-plugin": "^9.0.0",
    "glob": "^11.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "prettier": "^3.0.0",
    "replace-in-file-webpack-plugin": "^1.0.6",
    "sass": "^1.63.4",
    "sass-loader": "^16.0.0",
    "style-loader": "^4.0.0",
    "swc-loader": "^0.2.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.0.0",
    "webpack": "^5.86.0",
    "webpack-cli": "^5.1.4",
    "webpack-livereload-plugin": "^3.0.2"
  },
  "dependencies": {
    "@emotion/css": "^11.13.4",
    "@emotion/serialize": "^1.3.2",
    "@grafana/data": "^11.2.2",
    "@grafana/experimental": "^2.1.2",
    "@grafana/runtime": "^11.2.2",
    "@grafana/schema": "^11.2.2",
    "@grafana/ui": "^11.2.2",
    "lodash": "^4.17.21",
    "react": "^18.3.1",
